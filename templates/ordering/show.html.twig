{% extends 'base_admin.html.twig' %}

{% block title %}Ordering{% endblock %}

{% block body %}
    <h1>Ordering</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ ordering.id }}</td>
            </tr>
            <tr>
                <th>Product Name</th>
                <td>{{ ordering.productName }}</td>
            </tr>
            <tr>
                <th>Count</th>
                <td>{{ ordering.count }}</td>
            </tr>
            <tr>
                <th>Product Price</th>
                <td>{{ ordering.productPrice }}</td>
            </tr>
            <tr>
                <th>Total Price</th>
                <td>{{ ordering.totalPrice }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ ordering.email }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ ordering.telephone }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ ordering.name }}</td>
            </tr>
            <tr>
                <th>AddressStreet</th>
                <td>{{ ordering.addressStreet }}</td>
            </tr>
            <tr>
                <th>AddressCity</th>
                <td>{{ ordering.addressCity }}</td>
            </tr>
            <tr>
                <th>AddressZip</th>
                <td>{{ ordering.addressZip }}</td>
            </tr>
            <tr>
                <th>Notes</th>
                <td>{{ ordering.notes }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ ordering.status }}</td>
            </tr>

        </tbody>
    </table>

    <a href="{{ path('ordering_index') }}">back to list</a>

    <a href="{{ path('ordering_edit', {'id': ordering.id}) }}">edit</a>

    {{ include('ordering/_delete_form.html.twig') }}
{% endblock %}
